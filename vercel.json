{
    "redirects": [
        {
            "source": "/calendar",
            "has": [
                {
                    "type": "header",
                    "key": "user-agent",
                    "value": ".*(bot|crawler|spider|facebook|twitter|linkedin|whatsapp|telegram|slack|facebot|twitterbot|slackbot|telegrambot|LinkedInBot).*"
                }
            ],
            "destination": "/calendar-og.html",
            "permanent": false
        }
    ],
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.ts"
        },
        {
            "source": "/((?!.*\\.html$).*)",
            "destination": "/index.html"
        }
    ]
}