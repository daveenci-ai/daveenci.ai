{
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.ts"
        },
        {
            "source": "/(.*)",
            "destination": "/frontend/dist/$1",
            "continue": true
        },
        {
            "source": "/(.*)",
            "destination": "/frontend/dist/index.html"
        }
    ]
}